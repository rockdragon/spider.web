<% include common/header %>
<!-- Content Header (Page header) -->
<aside class="right-side" ng-app="statisticApp">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Statistic
            <small>spider</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Statistic</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content" ng-controller="statisticController as statCtrl">
        <h2 class="page-header">Houses count</h2>
        <div class="row">
            <div class="col-md-6">
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#soufun" data-toggle="tab">soufun</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="soufun">
                            <div class="small-box bg-green">
                                <div class="inner">
                                    <h3 ng-bind="statCtrl.data.count_soufun">
                                        0
                                    </h3>
                                    <p>
                                        Houses
                                    </p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="#" class="small-box-footer">
                                    <i class="fa fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div><!-- /.tab-pane -->
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#58" data-toggle="tab">58</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="58">
                            <div class="small-box bg-green">
                                <div class="inner">
                                    <h3 ng-bind="statCtrl.data.count_58">
                                        0
                                    </h3>
                                    <p>
                                        Houses
                                    </p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="#" class="small-box-footer">
                                    <i class="fa fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div><!-- /.tab-pane -->
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#anjuke" data-toggle="tab">anjuke</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="anjuke">
                            <div class="small-box bg-green">
                                <div class="inner">
                                    <h3 ng-bind="statCtrl.data.count_anjuke">
                                        0
                                    </h3>
                                    <p>
                                        Houses
                                    </p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="#" class="small-box-footer">
                                    <i class="fa fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div><!-- /.tab-pane -->
                    </div>
                </div>
            </div>
        </div>
    </section><!-- /.content -->
</aside><!-- /.right-side -->
<% include common/footer %>
