<% include common/header %>
<link href="/ng-grid/ng-grid.min.css" rel="stylesheet" type="text/css" />
<style type="text/css">
    .gridStyle {
        border: 1px solid rgb(212,212,212);
        width: 100%;
        height: 560px;
    }
</style>
<!-- Content Header (Page header) -->
<aside class="right-side" ng-app="sourceApp">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            <%=module%>
            <small>spider</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="/admin"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active"><%=module%></li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content" ng-controller="sourceController as sourceCtrl">
        <div class="gridStyle" ng-grid="gridOptions"></div>
        <input type="hidden" id="source" value="<%=module%>" />
        <input type="hidden" id="total" value="<%=total%>" />
    </section><!-- /.content -->
</aside><!-- /.right-side -->
<% include common/footer %>
<script src="/ng-grid/ng-grid-2.0.14.min.js"></script>
<script src="/adminlte/js/source.js"></script>