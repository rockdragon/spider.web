<% include common/header %>
<!-- Content Header (Page header) -->
<aside class="right-side" ng-app="houseApp">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            <%= module %>
            <small>spider</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="/admin"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active"><%= module %></li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content" ng-controller="houseController as houseCtrl">
        <div class="row">
            <section class="col-lg-6 connectedSortable ui-sortable">
                <!-- Map box -->
                <div class="box box-primary">
                    <div class="box-header" style="cursor: move;">
                        <i class="fa fa-map-marker"></i>
                        <h3 class="box-title">
                            {{ houseData.address }}
                        </h3>
                    </div>
                    <div class="box-body no-padding">
                        <div id="iMap" style="height:300px;"></div>
                    </div>
                    <!-- /.box-body-->
                </div>
                <!-- /.box -->
            </section>
        </div>

        <input type="hidden" id="houseData" value="<%= houseData %>"/>
    </section>
    <!-- /.content -->
</aside><!-- /.right-side -->
<% include common/footer %>
<script language="javascript" src="http://webapi.amap.com/maps?v=1.3&key=057024c38d35916d20e208c3bf6b2790"></script>
<script src="/adminlte/js/house.js"></script>